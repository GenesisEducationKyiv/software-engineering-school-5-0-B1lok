{
  "id": null,
  "uid": "subscription-service-dashboard",
  "title": "Subscription Service Metrics Dashboard",
  "timezone": "browser",
  "schemaVersion": 37,
  "version": 1,
  "refresh": "5s",
  "panels": [
    {
      "type": "stat",
      "title": "Active Subscriptions",
      "datasource": "Weather-Api Metrics",
      "targets": [
        {
          "expr": "subscription_service_active_subscriptions",
          "legendFormat": "Active Subscriptions",
          "refId": "A"
        }
      ],
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 8,
        "h": 8
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 100
              },
              {
                "color": "red",
                "value": 1000
              }
            ]
          }
        }
      }
    },
    {
      "type": "graph",
      "title": "Request Duration (95th Percentile)",
      "datasource": "Weather-Api Metrics",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(subscription_service_request_duration_seconds_bucket[5m]))",
          "legendFormat": "{{method}} - {{status}}",
          "refId": "A"
        }
      ],
      "gridPos": {
        "x": 8,
        "y": 0,
        "w": 8,
        "h": 8
      },
      "yAxes": [
        {
          "unit": "s"
        }
      ]
    },
    {
      "type": "graph",
      "title": "Request Duration (50th Percentile)",
      "datasource": "Weather-Api Metrics",
      "targets": [
        {
          "expr": "histogram_quantile(0.50, rate(subscription_service_request_duration_seconds_bucket[5m]))",
          "legendFormat": "{{method}} - {{status}}",
          "refId": "A"
        }
      ],
      "gridPos": {
        "x": 16,
        "y": 0,
        "w": 8,
        "h": 8
      },
      "yAxes": [
        {
          "unit": "s"
        }
      ]
    },
    {
      "type": "graph",
      "title": "Total Requests by Status Code",
      "datasource": "Weather-Api Metrics",
      "targets": [
        {
          "expr": "subscription_service_total_requests",
          "legendFormat": "{{method}} - {{status}}",
          "refId": "A"
        }
      ],
      "gridPos": {
        "x": 0,
        "y": 8,
        "w": 24,
        "h": 8
      }
    }
  ]
}